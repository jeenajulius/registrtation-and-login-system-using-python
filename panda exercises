        "df = pd.DataFrame({'grps': list('aaabbcaabcccbbc'), \n",
        "                   'vals': [12,345,3,1,45,14,4,52,54,23,235,21,57,3,87]})\n",
        "a,b,c=[],[],[]\n",
        "for i,j in zip(df.grps,df.vals):\n",
        "  if i=='a':\n",
        "    a.append(j)\n",
        "  elif i=='b':\n",
        "    b.append(j)\n",
        "  else:\n",
        "    c.append(j)\n",
        "a.sort(reverse=True)\n",
        "b.sort(reverse=True)\n",
        "c.sort(reverse=True)\n",
        "print(f'a:{sum(a[:3])}')\n",
        "print(f'b:{sum(b[:3])}')\n",
        "print(f'c:{sum(c[:3])}')"
